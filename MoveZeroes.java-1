  class Solution {
    public static void moveZeroes(int[] nums) {
        int n = nums.length;
        int count =0;
        int i=0,j=0;
        for(i=0;i<n;i++){
            if(nums[i]!=0){
                nums[count++]=nums[i];
            }
        }
        while(count<n){
            nums[count++]=0;
        }
    }
}

